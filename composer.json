{
    "name": "run_as_root/rooter",
    "description": "rooter is a local environment manager",
    "type": "project",
    "authors": [
        {
            "name": "Matthias Walter",
            "email": "matthias.walter@run-as-root.sh"
        }
    ],
    "require": {
        "ext-posix": "*",
        "symfony/config": "^6.3",
        "symfony/console": "^6.3",
        "symfony/dependency-injection": "^6.3",
        "symfony/process": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "bin": [
        "rooter"
    ],
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist",
        "sort-packages": true,
        "vendor-dir": "vendor",
        "allow-plugins": {
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "RunAsRoot\\Rooter\\": "src"
        }
    }
}
